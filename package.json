{
  "name": "expressjs_prisma_ts_crud",
  "version": "1.0.0",
  "description": "An ExpressJS CRUD REST API with Typescript and Prisma",
  "main": "./dist/server.js",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "ts-start": "ts-node-dev src/server.ts",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\"  \"nodemon --inspect=5151 --inspect-brk dist/server.js\"",
    "dev:serve": "npm-run-all --parallel ts-start serve",
    "prisma:push": "npx prisma db push",
    "prisma:init": "npx prisma init --datasource-provider postgresql",
    "prisma:pull": "prisma db pull",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "npx prisma db seed"
  },
  "author": "Beau Iyere",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.16.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/nodemon": "^1.19.2",
    "@types/pg": "^8.10.2",
    "@types/sequelize": "^4.28.15",
    "@types/uuid": "^9.0.2",
    "concurrently": "^8.2.0",
    "esbuild-register": "^3.4.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.16.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
